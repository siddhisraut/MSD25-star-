#include <iostream>
using namespace std;

class Distance {
public:
    int feet, inch;
    Distance(int f, int i) {
        this->feet = f;
        this->inch = i;
    }
    void operator-() {
        feet--;
        inch--;
    }
    void display() {
        cout << "\nFeet & Inches (Decrement): " << feet << " " << inch << endl;
    }
};

